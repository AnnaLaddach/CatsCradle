citHeader("To cite CatsCradle in publications use:")

bibentry(
bibtype    = 'Misc',
title    = 'CatsCradle',
author   = c(person(given='Anna',family='Laddach'),
             person(given='Michael',family='Shapiro')),
year     = '2024',
url      = 'https://github.com/AnnaLaddach/CatsCradle',
textVersion = paste('Anna Laddach and Michael Shapiro,',
                    'CatsCradle: an R package for spatial',
                    'single cell transcriptomics and the',
                    'clustering and annotation of genes,',
                    'developed in the Pachnis lab at the',
                    'Francis Crick Institute',
                    'https://github.com/AnnaLaddach/CatsCradle')
)

